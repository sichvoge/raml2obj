#%RAML 1.0
title: Test
description: Test of array support
version: v1
baseUri:
  value: https://exmaple.com/{version}
protocols: HTTPS
mediaType: application/json

types:
  Foo:
    properties:
      id: string
  Foos: Foo[]

/foos:
  get:
    responses:
      200:
        body: Foos
  post:
    responses:
      200:
        body:
          type: array
          items: Foo
